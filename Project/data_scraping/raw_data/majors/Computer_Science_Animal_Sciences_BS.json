    "learning_outcomes": [
      {
        "type": "paragraph",
        "content": "for the degree of Bachelor of Science Major in Computer Science + Animal Sciences"
      },
      {
        "type": "paragraph",
        "content": "Students will be prepared to work with sensor technology, large data sets, and predictive analytics, all aimed at improving the health and well-being of production animals and pets."
      }
