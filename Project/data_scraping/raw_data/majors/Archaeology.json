    "learning_outcomes": [
      {
        "type": "paragraph",
        "content": "for the degree of Bachelor of Arts in Liberal Arts and Sciences Major in Anthropology, Archaeology Concentration"
      },
      {
        "type": "paragraph",
        "content": "Students who graduate with a BALAS degree in Anthropology will have:"
      },
      {
        "type": "list",
        "list_type": "ol",
        "content": [
          "A broad knowledge of cultural, social, linguistic, archaeological, and biological facets of the human condition and the methods anthropologists use to study them.",
          "An understanding of the ethical and social dimensions of anthropological research and their impacts on society.",
          "The ability to conduct independent research through data collection, critical analysis, synthesis, and written presentation of findings."
        ]
      },
      {
        "type": "paragraph",
        "content": "Students who graduate with a concentration in Archaeology will have:"
      },
      {
        "type": "list",
        "list_type": "ol",
        "content": [
          "Knowledge of the theories and methods for understanding cultural and societal change through time and the role of heritage in the present.",
          "An understanding of the ethical issues and legal responsibilities concerning cultural resource management."
        ]
      }
    ],
